{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{version}/price/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/{version}/price/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "ServiceName": "price",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "DownstreamPathTemplate": "/api/{version}/trades/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/{version}/trades/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "ServiceName": "trade",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "DownstreamPathTemplate": "/api/{version}/trades",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/{version}/trades",
      "UpstreamHttpMethod": [ "GET", "PUT" ],
      "ServiceName": "trade",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "ServiceDiscoveryProvider": {
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}